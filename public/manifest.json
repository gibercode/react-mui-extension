{
  "manifest_version": 3,
  "name": "My Preact Material Extension",
  "version": "1.0",
  "options_page": "options.html",
  "background": {
    "service_worker": "dist/background.js"
  },
  "action": {},
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": ["https://*/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"]
    }
  ]
}
